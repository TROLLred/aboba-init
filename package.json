{
  "name": "a101-mini-app",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "engines": {
    "npm": ">=9.6.0",
    "node": ">=20.3.0"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "type-check": "tsc --noEmit",
    "husky:validateBranchName": "node scripts/husky/validateBranchName.js",
    "prettier:fix": "prettier . --write",
    "prettier:check": "prettier --check ./src",
    "prepare": "husky install",
    "all-checks": "npm run lint:ts && npm run lint:scss && npm run type-check && npm run husky:validateBranchName && npm run prettier:check"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ],
    "*.(scss)": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@domyland-shared/mini-app-sdk": "git+https://gitlab.domyland.dev/domyland-shared/mini-app-sdk",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/react-transition-group": "^4.4.10",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "jsbarcode": "^3.11.6",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-transition-group": "^4.4.5",
    "swiper": "^11.1.4",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "stylelint": "^16.6.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.2.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
